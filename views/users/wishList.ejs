    <section class="light section_gap">
        <div class="container py-2">
            <div class="h1 text-center text-dark" id="pageHeaderTitle">‚ù§Ô∏è Favorite ‚ù§Ô∏è</div>

            <h3 class="text-center" style="color: black;">Explore Your Next Destination</h3>

            <% PasswishData.wishlist.forEach((ResortElements)=>{ %>

                <article class="postcard light yellow">
                    <a class="postcard__img_link" href="#">
                        <img class="postcard__img"
                            src="/resortImages/<%= ResortElements.ResortId.resortsOtherImages[0]%>" alt="Image Title" />
                    </a>
                    <div class="postcard__text t-dark">
                        <h1 class="postcard__title yellow"><a href="#">
                                <%=ResortElements.ResortId.resortName%>
                            </a></h1>
                        <div class="postcard__subtitle small">
                            <time datetime="2020-05-25 12:00:00">
                                <i class="fas fa-calendar-alt mr-2"></i>Mon, May 25th 2020
                            </time>
                        </div>

                        <div class="postcard__bar"></div>
                        <div class="postcard__preview-txt">
                            <%=ResortElements.ResortId.resortDiscription%>
                        </div>
                        <h3 class="mb-1 me-1" style="font-weight:500;color: rgb(111, 111, 249);">‚Çπ
                            <%=ResortElements.ResortId.resortPrice %>
                        </h3>

                        <ul class="postcard__tagbox">
                            <li class="tag__item  button_hover ">
                              <a href="/open_resort/<%=ResortElements.ResortId._id%>"><i class=" mr-2"></i>View Details</a>
                            </li>
                            <li class="tag__item button_hover2">
                                <a href="/removewishlist/<%= ResortElements.ResortId._id%>"><i class=" mr-2"
                                        style="font-size:20px ;">üíî</i> Remove </a>
                            </li>
                        </ul>
                    </div>
                </article>
                <%})%>

        </div>
    </section>


