  <section class="about_history_area section_gap">
    <div class="container">
      <div class="row">
        <div class="col-md-5 col-lg-4 col-xl-3 col-6 col-sm-7  align-items-center ">


          <article class="card-group-item">



            <div class="filter-content" style="background-color: #04091e;border-radius: 10px">

              <header class="card-header ">
                <h4 class="" style="color: rgb(249, 248, 248);">SEARCH </h4>
              </header>
              <div class="card-body">
                <form action="">
                  <div class="col-md-12 ">
                    <form action="" method="post">
                      <div class="form-group">
                        <label for="location">Location</label>
                        <input type="text" class="form-control" placeholder="Enter a location" name="search"
                          value="">
                      </div>
                      <div class="form-group">
                        <label for="checkin">Check-in</label>
                        <input type="date" class="form-control" id="checkin" placeholder="Check-in">
                      </div>
                      <div class="form-group">
                        <label for="checkout">Check-out</label>
                        <input type="date" class="form-control" id="checkout" placeholder="Check-out">
                      </div>
                      <div class="input-group">

                        <select class="wide mt-4" name="">
                          <option data-display="Rooms">Number of Rooms</option>
                          <option value="1">Room 01</option>
                          <option value="2">Room 02</option>
                          <option value="3">Room 03</option>
                        </select>
                      </div>
                      <div class="input-group">

                        <select class="wide mt-4">
                          <option data-display="Adult">Adult</option>
                          <option value="1">Old</option>
                          <option value="2">Younger</option>
                          <option value="3">Potato</option>
                        </select>
                      </div>
                      <div class="input-group">

                        <select class="wide mt-4 mb-4">
                          <option data-display="Child">Child</option>
                          <option value="1">Child</option>
                          <option value="2">Baby</option>
                          <option value="3">Child</option>
                        </select>
                      </div>

                      <button type="submit" class="btn btn-primary book_now_btn button_hover"
                        style="background-color: #f3c300;border: none; box-shadow: none;">Search</button>
                    </form>
                  </div>


              </div> <!-- card-body.// -->
            </div>
           
          </article>

        </div>


        <div class="col-md-7 col-lg-8 col-6 col-xl-9 col-sm-5 ">
          <% ResortElements.forEach((ResortElements)=>{ %>

            <div class="row justify-content-center mb-3">
              <div class="col-md-12 col-xl-12 ">
                <div class="card" style="border:solid 0.2px;border-radius:7px;box-shadow: none; ">
                  <div class="card-body ">
                    <div class="row">
                      <div class="col-md-12 col-lg-3 col-xl-3 mb-4 mb-lg-0">
                        <div class="bg-image hover-zoom ripple rounded ripple-surface">
                          <img src="/resortImages/<%= ResortElements.resortsOtherImages[0]%>" class="w-100"
                            style="height: 11rem; width: 12rem;" />
                          <a href="#!">
                            <div class="hover-overlay">
                              <div class="mask" style="background-color: rgb(255, 255, 255);"></div>
                            </div>
                          </a>
                        </div>
                      </div>
                      <div class="col-md-6 col-lg-6 col-xl-6">
                        <h4 class=""
                          style="color: #021251; font-family: Arial, Helvetica, sans-serif; font-weight: 600;">
                          <%=ResortElements.resortName %>
                        </h4>
                        <div class="d-flex flex-row">
                          <div class="text-danger mb-1 me-2">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                          </div>
                          <span>310</span>
                        </div>
                        <div class="mt-1 mb-0 text-muted small">
                          <span>100% cotton</span>
                          <span class="text-primary"> • </span>
                          <span>Light weight</span>
                          <span class="text-primary"> • </span>
                          <span>Best finish<br /></span>
                        </div>
                        <div class="mb-2 text-muted small">
                          <span>Unique design</span>
                          <span class="text-primary"> • </span>
                          <span>For men</span>
                          <span class="text-primary"> • </span>
                          <span>Casual<br /></span>
                        </div>
                        <p class="text-truncate mb-4 mb-md-0">
                          <%=ResortElements.resortDiscription %>
                        </p>
                      </div>
                      <div class="col-md-6 col-lg-3 col-xl-3 border-sm-start-none border-start">
                        <div class="d-flex flex-row align-items-center mb-1">
                          <h4 class="mb-1 me-1">₹ <%=ResortElements.resortPrice %>
                          </h4>
                          <!-- <span class="text-danger"><s>$20.99</s></span> -->
                        </div>
                        <!-- <h6 class="text-success">Free shipping</h6> -->
                        <div class="d-flex flex-column mt-4">
                          <a href="/open_resort/<%=ResortElements._id%>"> <button class="btn  btn-sm button_hover w-100"
                              type="button"
                              style="background-color: #04091e;color: aliceblue; box-shadow: none;  border-radius: 4px ;font-size: 20px">Details</button></a>
                          <a href="/AddTOwishList/<%= ResortElements._id%>"> <button id="wishlistBtn"
                              class="btn btn-sm mt-2 button_hover2 w-100" type="button"
                              style="box-shadow: none; border-radius: 4px; font-size: 20px;">
                              ❤️ Add to wishlist
                            </button></a>

                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <%})%>




            <nav class="blog-pagination justify-content-center d-flex">
              <ul class="pagination">
                  <li class="page-item">
                      <a href="#" class="page-link" aria-label="Previous">
                          <span aria-hidden="true">
                              <span class="lnr lnr-chevron-left"></span>
                          </span>
                      </a>
                  </li>
                  <% for(let j=1;j<=totelPage;j++){ %>

                  <li class="page-item"><a href="?page=<%= j %>" class="page-link">              
                            <%= j %>
                  </a></li>
                  <% }%>

                
                  <li class="page-item">
                      <a href="?page=<%=nextPage%>" class="page-link" aria-label="Next">
                          <span aria-hidden="true">
                              <span class="lnr lnr-chevron-right"></span>
                          </span>
                      </a>
                  </li>
              </ul>
          </nav>


            
              <!-- <img class="img-fluid" src="/imageFront/about_bg.jpg" alt="img"> -->
        </div>

      </div>
    </div>
  </section>
