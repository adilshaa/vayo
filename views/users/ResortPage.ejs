                                <h4 class="" style="color: rgb(249, 248, 248);">SEARCH </h4>
                            </header>
                            <div class="card-body">

                                <div class="col-md-12 ">
                                    <form action="/search">
                                        <div class="form-group">
                                            <i class="mdi mdi-map-marker"></i> 
                        <label for="location">Location</label>
                                            <input type="text" class="form-control" placeholder="Enter a location"
                                                name="search" value="">
                                        </div>
                                        <div class="form-group">
                                            <i class="mdi mdi-calendar-text"></i> 
                                            <label for="checkin">Check-in</label>
                                            <input type="date" class="form-control" id="checkin" placeholder="Check-in">
                                        </div>
                                        

                                        <button type="submit" class="btn btn-primary book_now_btn button_hover"
                                            style="background-color: #f3c300;border: none; box-shadow: none;">Search</button>
                                    </form>
                                </div>


                            </div> <!-- card-body.// -->
                        </div>


                    </article>

                </div>


                <div class="col-md-6 col-lg-9 col-6 col-xl-9 col-sm-12 col-12">
                    <div class="d-flex flex-column mt-4">
                        <div class="h3" style="color: black; font-weight: 600;" id="">
                            <%=passResortDatas.resortName%>
                        </div>
                    </div>

                    <h5 class="  text-primary">
                        <i class="mdi mdi-map-marker"></i>  <%=passResortDatas.resortLocation %>
                    </h5>






                    <div id="carouselExampleControls4" class="carousel slide" data-ride="carousel">
                        <div class="carousel-inner">
                            <% for (let i=0; i < passResortDatas.resortsOtherImages.length; i+=2) { %>
                                <div class="carousel-item <%= i === 0 ? 'active' : '' %>">
                                    <div class="row m_30 ">


                                        <div class="col-lg-6 col-sm-6 col-12">

                                            <img src="/resortImages/<%= passResortDatas.resortsOtherImages[i]%>"
                                                class="card-img-top" alt="..." style="width: 27rem; height: 20rem;">
                                        </div>
                                        <% if (i+1 < passResortDatas.resortsOtherImages.length) { %>

                                            <div class="col-lg-6 col-sm-6 col-12">
                                                <img src="/resortImages/<%= passResortDatas.resortsOtherImages[i+1]%>"
                                                    class="card-img-top" alt="..." style="width: 27rem; height: 20rem;">
                                            </div>
                                            <div class="hover">
                                                <a class="light" href="/resortImages/<%= passResortDatas.resortsOtherImages[i+1]%>"><i class="fa fa-expand"></i></a>
                                            </div>

                                            <% } %>
                                    </div>
                                </div>
                                <% } %>
                        </div>
                        <a class="carousel-control-prev"  href="#carouselExampleControls4" role="button"
                            data-slide="prev">
                            <i class="fa fa-chevron-left" aria-hidden="true" style="color: black;"></i>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleControls4" role="button"
                            data-slide="next">
                            <i class="fa fa-chevron-right" aria-hidden="true" style="color: black;"></i>

                            <span class="sr-only">Next</span>
                        </a>
                    </div>

                    <div class="image-row mt-3">
                        <% for (let i=0; i < passResortDatas.resortsOtherImages.length; i++) { %>
                            <% if ( i<=10){ %>
                                <img src="/resortImages/<%= passResortDatas.resortsOtherImages[i]%>" alt="Image 1"
                                    style="height: 100px;">
                                <% } %>
                                    <% } %>
                    </div>
                    <!-- <img class="img-fluid" src="/imageFront/about_bg.jpg" alt="img"> -->
                </div>

            </div>
        </div>
        </div>
