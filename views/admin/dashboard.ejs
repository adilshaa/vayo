                  <script>
                    document.addEventListener("DOMContentLoaded", () => {
                      new Chart(document.querySelector('#categorysales'), {
                        type: 'bar',
                        data: {
                          labels: ['<%=categoryName[0]%>','<%=categoryName[1]%>','<%=categoryName[2]%>','<%=categoryName[3]%>','<%=categoryName[4]%>','<%=categoryName[5]%>'],
                          datasets: [{
                            label: 'Sales Count',
