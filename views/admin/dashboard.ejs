                              'rgb(251, 17, 0)',
                              'rgb(234, 251,0)',
                              'rgb(0, 229, 255)',
                              'rgb(50, 251, 0)',

                              

                              'rgb(54, 162, 235)',

                              'rgb(54, 162, 235)',

                            ],
                            borderColor: [
                            'rgb(251, 17, 0)',
                              'rgb(234, 251,0)',
                              'rgb(0, 229, 255)',
                              'rgb(50, 251, 0)',

                              'rgb(54, 162, 235)',

                              'rgb(54, 162, 235)',

                              'rgb(54, 162, 235)',

                            ],
                            borderWidth: 1
                          }]
                        },
                        options: {
                          scales: {
                            yAxes: [{
                              ticks: {
                                beginAtZero: true
                              }
                            }]
                          }
                        }
                      });
                    });
                  </script>
                  <!-- End Bar Chart -->
            
                </div>
              </div>
            </div>
            



                  <script>
                  

                    let COD=document.getElementById("COD").value
