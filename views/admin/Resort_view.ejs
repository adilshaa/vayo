                  <div class="table-responsive">
                    <table id="recent-purchases-listing" class="table">
                      <tr>
                        <th scope="col">Id</th>
                        <th scope="col">Resort Name</th>
                        <th scope="col">Resort Discription</th>
                        <th scope="col"> Resort Location </th>
                        <th scope="col">Resort Images </th>
                        <th scope="col"> Resort Price </th>
                        <th scope="col"> Resort Category </th>
                        <th scope="col"> Resort Policies </th>
                      </tr>
                      </thead>
                      <tbody>
                        <% passResortData.forEach((elements,index)=>{ %>
                          <tr>
                            <th scope="row">
                              <%=index+1 %>
                            </th>
                            <td>
                              <%= elements.resortName %>
                            </td>
                            <td>
                              <%= elements.resortDiscription %>
                            </td>
                            <td>
                              <%= elements.resortLocation %>
                            </td>
                            <td><img src="/resortImages/<%=elements.resortsOtherImages[0]%>" alt="" width="70px"
                                height="70px"> </td>
                            <td> ₹ <%= elements.resortPrice %>
                            </td>
                            <td>
                              <%= elements.category.category%>
                            </td>
                            <td>
                              <%= elements.resortPolicies %>
                            </td>
                            <form action="/admin/View_resort/deletecategory/<%=elements._id%>" method="get">
                              <td> <button type="submit" class="btn btn-danger">delete</button>
                            </form>
                            </td>
                            <td>
                              <form action="/admin/View_resort/editresort/<%=elements._id%>" method="get">
                                <button type="submit" class="btn btn-danger">Edit</button>
                              </form>
                            </td>
                            <td>
                              <form action="/admin/View_resort/List/<%=elements._id%>" method="get">

                                <% if(elements.resortStatus==true){ %>
                                  <button type="submit" class="btn btn-danger">List</button>
                                  <% }else{ %>
                                    <button type="submit" class="btn btn-danger">Unlist</button>
                                    <% } %>
                              </form>
                            </td>

                          </tr>
                          <% }); %>

                      </tbody>
