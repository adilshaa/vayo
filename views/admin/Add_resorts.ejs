          <div class="col-9 grid-margin stretch-card">
            <div class="card">
              <div class="card-body">
                <h3>ADD RESORT</h3>
                <p class="card-description">
                  Basic form elements
                </p>
                <form class="forms-sample" action="" method="post" enctype="multipart/form-data">
                  <div class="form-group">
                    <label for="exampleInputName1">Resort Name</label>
                    <input type="text" class="form-control" id="exampleInputName1" name="resortName" placeholder=""
                      required>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputEmail3">Resort Location</label>
                    <input type="text" class="form-control" id="exampleInputEmail3" placeholder="" name="resortLocation"
                      required>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputPassword4">Price</label>
                    <input type="number" class="form-control" id="exampleInputPassword4" placeholder=""
                      name="resortPrice" required>
                  </div>
                  <div class="form-group ">
                    <select class="form-control w-50" name="category" type="text" required>
                      <%passCatergory.forEach((elements,index)=>{ %>
                        <% if(elements.categoryStatus==true){ %>
                          <option value="<%=elements._id %>">
                            <%=elements.category %>
                          </option>
                          <% } %>
                            <% }) %>
                    </select>

                  </div>

                  <div class="form-group">
                    <label>Add Resort Images</label>
                    <input multiple type="file" name="resortsOtherImages" class="file-upload-default">
                    <div class="input-group col-xs-12">
                      <input type="text" class="form-control file-upload-info" disabled placeholder="Upload Image"
                        required>
                      <span class="input-group-append">
                        <button class="file-upload-browse btn btn-primary" type="button">Upload</button>
                      </span>
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputCity1">Discription About Resort</label>
                    <input type="text" class="form-control" id="exampleInputCity1" name="resortDiscription"
                      placeholder="" required>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputCity1">Number Of Rooms</label>
                    <input type="text" class="form-control" id="exampleInputCity1" name="resortRooms"
                      placeholder="" required>
                  </div>
                   <div class="form-group">
                    <label for="exampleInputCity1">Availability Data</label>
                    <input type="date" class="form-control" id="exampleInputCity1" name="resortAvailabilityData" placeholder=""
                      required>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputCity1">Resort Policies</label>
                    <input type="text" class="form-control" id="exampleInputCity1" name="resortPolicies" placeholder=""
                      required>
                  </div>
                  <% if(typeof message !=="undefined" ) { %>
                    <h5 style="font-size: 14px;color: brown;">
                      <%=message %>
                    </h5>
                    <%}%>

                  <button type="submit" class="btn btn-primary mr-2">Submit</button>
                  <button class="btn btn-light">Cancel</button>
                </form>
              </div>
            </div>
          </div>
