          <div class="col-9 grid-margin stretch-card">
            <div class="card">
              <div class="card-body">
                <h3>ADD RESORT</h3>
                <p class="card-description">
                  Basic form elements
                </p>
                <form class="forms-sample" action="" method="post">
                  <div class="form-group">
                    <label for="exampleInputName1">Resort Name</label>
                    <input type="text" class="form-control" id="exampleInputName1"
                      value="<%=passResortData.resortName%>" name="resortName" placeholder="" required>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputEmail3">Resort Location</label>
                    <input type="text" class="form-control" id="exampleInputEmail3"
                      value="<%=passResortData.resortLocation%>" placeholder="" name="resortLocation" required>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputPassword4">Price</label>
                    <input type="number" class="form-control" id="exampleInputPassword4" placeholder=""
                      value="<%=passResortData.resortPrice%>" name="resortPrice" required>
                  </div>
                  <div class="form-group ">
                    <select class="form-control w-50" name="category" type="text" required>
                      <option>Resort Categories</option>
                      <option value="<%=passResortData.category._id %>">
                        <%=passResortData.category.category%>
                      </option>
                      <%PassCategoryData.forEach((elements)=>{ %>
                        <% if(elements.categoryStatus==true){ %>
                          <option value="<%=elements._id%>">
                            <%=elements.category%>
                          </option>
                          <% } %>
                            <% }) %>
                    </select>
                  </div>

                  <div class="form-group">

                    <div class="input-group col-xs-12">
                      <label for="exampleInputEmail3">EDIT IMAGE</label>

                      <span class="input-group-append">
                        <button class=" btn btn-secondary ml-3 " type="button" data-toggle="modal"
                          data-target="#myModal">EDIT</button>
                      </span>
                    </div>
                  </div>

                  <div class="row">
                    <div class="d-flex  col-3 col-lg-6 ">

                      <% passResortData.resortsOtherImages.forEach((Imgelements)=>{ %>

                        <img src="/resortImages/<%=Imgelements %>" alt="" style="width: 7rem">


                        <% }) %>
                    </div>
                  </div>



                  <div class="form-group mt-3">
                    <label for="exampleInputCity1">Discription About Resort</label>
                    <input type="text" class="form-control" id="exampleInputCity1"
                      value="<%=passResortData.resortDiscription%>" name="resortDiscription" placeholder="">
                  </div>

                  <div class="form-group">
                    <label for="exampleInputCity1">Number Of Rooms</label>
                    <input type="text" class="form-control" id="exampleInputCity1" name="resortRooms" value="<%=passResortData.resortRooms%>"
                      placeholder="" required>
                  </div>
                   <div class="form-group">
                    <label for="exampleInputCity1">Availability Data</label>
                    <input type="date" class="form-control" id="exampleInputCity1" name="resortAvailabilityData" placeholder="" value="<%=passResortData.resortAvailabilityData%>"
                      required>
                  </div>


                  <div class="form-group">
                    <label for="exampleInputCity1">Resort Policies</label>
                    <input multiple type="text" class="form-control" id="exampleInputCity1"
                      value="<%=passResortData.resortPolicies%>" name="resortPolicies" placeholder="">
                  </div>
                  <% if(typeof message !=="undefined" ) { %>
                    <h5 style="font-size: 14px;color: brown;">
                      <%=message %>
                    </h5>
                    <%}%>

                  <button type="submit" class="btn btn-primary mr-2">Submit</button>
                </form>
              </div>
            </div>
          </div>






          <!-- modal -->
          <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog" role="document">
              <div class="modal-content">

                <!-- Modal Header -->
                <div class="modal-header">
                  <h5 class="modal-title" id="myModalLabel">Modal Header</h5>
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
                <!-- Modal Body -->
                <div class="modal-body">
                  <form action="/admin/View_resort/editimage/<%=passResortData._id %>" method="post"
                    enctype="multipart/form-data">
                    <input multiple type="file" name="resortsOtherImages">

                    <% passResortData.resortsOtherImages.forEach((dt)=>{ %>
                      <div style="background-color: rgb(255, 255, 255);width: 100%;height: 80px;border: solid 0.1px;">
                        <img style="width: 130px;" src="/resortImages/<%- dt %>" alt="">

                        <a href="/admin/deleteimage/<%= dt %>/<%- passResortData._id%> " style="float: right;"
                          class="btn btn-danger my-3">delete</a>
                      </div>
                      <% }) %>

                </div>

                <!-- Modal Footer -->
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                  <button type="submit" class="btn btn-secondary">Save Changes</button>

                  </form>
                </div>
              </div>
            </div>
          </div>

